@{ ViewData["Title"] = "Index";
                Layout = "~/Views/Shared/_Layout.cshtml"; }

<div class="container-fluid">
    <div class="row header_section mx-0 px-0 border_bottom">
        <div class="col-5 position-relative ps-0">
            <span class="btn btn-light float-start mt-1 me-2 d-block d-md-none" id="menu_toggle">
                <i class="fa-solid fa-bars"></i>
            </span>
            <h3 class="mb-0">Loan Processing List</h3>
        </div>
        <p class="mb-0 col-7 top_link_Section"><a href="@Url.Action("Index","Disbursment",new { Area="GPF" })" class="">GPF Disbursment Dashboard</a><span class="px-1">/</span><span>Loan Processing Dashboard</span></p>
    </div>
    <div class="container-fluid pd-x-0 pd-lg-x-10 pd-xl-x-0">
        <div class="row row-xs pt-3">


            <div class="col-sm-6 col-lg-3 mt-2">
                <div class="card dashboard background-blue">
                    <a href="@Url.Action("RefundableList","Disbursment",new {Area="GPF"})">
                        <div class="card-body">
                            <div class="d-flex d-lg-block d-xl-flex align-items-end">
                                <h5 class="tx-normal tx-rubik mg-b-0 mg-r-5 lh-1 box_title">Refundable</h5>
                                <i class="fa-solid fa-house-chimney-medical"></i>
                            </div>
                        </div>
                    </a>
                </div>
            </div><!-- col -->
            <div class="col-sm-6 col-lg-3 mt-2">
                <div class="card dashboard background-yellow">
                    <a href="@Url.Action("NonRefundableList","Disbursment",new {Area="GPF"})">
                        <div class="card-body">
                            <div class="d-flex d-lg-block d-xl-flex align-items-end">
                                <h5 class="tx-normal tx-rubik mg-b-0 mg-r-5 lh-1 box_title">Non-Refundable</h5>
                                <i class="fa-solid fa-list"></i>
                            </div>
                        </div>
                    </a>
                </div>
            </div><!-- col -->
        </div>
    </div>



</div>