@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<!-- Content Wrapper. Contains page content -->


    <div class="container-fluid">

        <div class="row header_section mx-0 px-0 border_bottom">
            <div class="col-5 position-relative ps-0">
                <span class="btn btn-light float-start mt-1 me-2 d-block d-md-none" id="menu_toggle">
                    <i class="fa-solid fa-bars"></i>
                </span>
                <h3 class="mb-0">GPF Dashboard</h3>
            </div>
            <p class="mb-0 col-7 top_link_Section"><a class="" href="@Url.Action("Index","Home", new { Area="" })">GPF Dashboard </a></p>
        </div>
        <div class="container-fluid pd-x-0 pd-lg-x-10 pd-xl-x-0">
            <div class="row row-xs">



                <div class="col-sm-6 col-lg-3 mt-2">
                    <div class="card dashboard background-yellow">
                        <a href="@Url.Action("Index","Processing", new { Area="GPF" })">
                            <div class="card-body">
                                <div class="d-flex d-lg-block d-xl-flex align-items-end">
                                    <h5 class="tx-normal tx-rubik mg-b-0 mg-r-5 lh-1 box_title">GPF Processing</h5>
                                    <i class="fa-solid fa-list"></i>
                                </div>
                            </div>
                        </a>
                    </div>
                </div><!-- col -->
                <div class="col-sm-6 col-lg-3 mt-2">
                    <div class="card dashboard background-skyBlue">
                        <a href="@Url.Action("Index","Withdrawal", new { Area="GPF" })">
                            <div class="card-body">
                                <div class="d-flex d-lg-block d-xl-flex align-items-end">
                                    <h5 class="tx-normal tx-rubik mg-b-0 mg-r-5 lh-1 box_title">GPF withdrawal </h5>
                                    <i class="fa-solid fa-exclamation"></i>
                                </div>
                            </div>
                        </a>
                    </div>
                </div><!-- col -->

                <div class="col-sm-6 col-lg-3 mt-2">
                    <div class="card dashboard background-purple">
                        <a href="#">
                            <div class="card-body">
                                <div class="d-flex d-lg-block d-xl-flex align-items-end">
                                    <h5 class="tx-normal tx-rubik mg-b-0 mg-r-5 lh-1 box_title">Edlis</h5>
                                    <i class="fa-solid fa-check-to-slot"></i>
                                </div>
                            </div>
                        </a>
                    </div>
                </div><!-- col -->

            </div>
        </div>



    </div>
