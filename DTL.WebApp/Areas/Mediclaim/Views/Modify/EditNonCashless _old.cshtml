                                    <div class="col-md-2">
                                        <label class="w-100 form-label text-right">Name of Hospital</label>
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" class="form-control mb-2" placeholder="Enter Name of Hospital" id="txtIPDHospitalName" />
                                    </div>
                                    <div class="col-md-2">
                                        <label class="w-100 form-label text-right">Treatment Illness</label>
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" class="form-control mb-2" placeholder="Enter Treatment Illness" id="txtTreatment" />
