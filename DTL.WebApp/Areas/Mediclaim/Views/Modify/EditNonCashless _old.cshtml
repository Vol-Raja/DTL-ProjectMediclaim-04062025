@using DTL.Model.Models
@model IEnumerable<MasterDocumentModel>
@{
    ViewData["Title"] = "NonCashless";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="content-wrapper" style="margin:0px">
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0">Non-Cashless Mediclaim Form</h1>
                </div><!-- /.col -->
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="@Url.Action("Index","Home",new { Area="" })">Dashboard</a></li>
                        <li class="breadcrumb-item"><a href="@Url.Action("Index","Dashboard",new { Area="Mediclaim" })">Mediclaim</a></li>
                        <li class="breadcrumb-item"><a href="@Url.Action("Index","Noncashless",new { Area="Mediclaim" })">NonCashless List</a></li>
                        <li class="breadcrumb-item active">Raise</li>
                    </ol>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->
    <!-- Main content -->
    <section class="content">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="card">
                        <div class="card-body">

                            <form id="addMediclaimNonCashless">
                                <input type="hidden" id="hdnMediClaimId" value="@ViewBag.ClaimId" />
                                <h4 class="bg-gradient-gray p-1 mb-3">Main Information</h4>
                                <div class="row">
                                    <div class="col-md-2">
                                        <label class="w-100 form-label text-right">Employee Number</label>
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" class="form-control mb-2" placeholder="Enter Employee Number" id="txtEmployeeNumber" />
                                    </div>
                                    <div class="col-md-2">
                                        <label class="w-100 form-label text-right">PPO No.</label>
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" class="form-control mb-2" placeholder="Enter PPO Number" id="txtPPONumber" />
                                    </div>
                                    <div class="col-md-2">
                                        <label class="w-100 form-label text-right">Medical section Page No</label>
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" class="form-control mb-2" placeholder="Enter Medical section Page Number" id="txtMedicalSectionPageNumber" />
                                    </div>
                                    <div class="col-md-2">
                                        <label class="w-100 form-label text-right">Name of Medical Card Holder</label>
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" class="form-control mb-2" placeholder="Enter Medical Card Number" id="txtMedicalCardHolderName" />
                                    </div>
                                    <div class="col-md-2">
                                        <label class="w-100 form-label text-right">Designation</label>
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" class="form-control mb-2" placeholder="Enter Designation" id="txtDesignation" />
                                    </div>
                                    <div class="col-md-2">
                                        <label class="w-100 form-label text-right">Name of Patient</label>
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" class="form-control mb-2" placeholder="Enter Paitent Name" id="txtPatientName" />
                                    </div>


                                    <div class="col-md-2">
                                        <label class="w-100 form-label text-right">Gender</label>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="gender-radio">
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" name="gender" id="rdMale" value="Male">
                                                <label class="form-check-label" for="male">Male</label>
                                            </div>
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" name="gender" id="rdFemale" value="Female">
                                                <label class="form-check-label" for="female">Female</label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <label class="w-100 form-label text-right">Date of Birth</label>
                                    </div>
                                    <div class="col-md-2">
                                        <input type="date" class="form-control mb-2" id="txtDateOfBirth" />
                                    </div>
                                    <div class="col-md-1">
                                        <label class="w-100 form-label text-right">Age</label>
                                    </div>
                                    <div class="col-md-1">
                                        <input type="text" class="form-control mb-2" id="txtAge" />
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-2">
                                        <label class="w-100 form-label text-right">Dependent/Self</label>
                                    </div>
                                    <div class="col-md-4 mb-4">
