@using DTL.Model.Models.Mediclaim
@model ClaimAndVoucherDetailModel
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .top_print_btn {
        position: absolute;
        right: 20px;
        top: 30px;
    }

    .view_container_bg {
        background: rgb(0 43 255 / 4%);
    }

    .view_table tr td {
        color: #005ccf;
    }
</style>

<div class="container-fluid view_form">
    <div class="row header_section mx-0 px-0">
        <div class="col-6 position-relative ps-0">
            <span class="btn btn-light float-start mt-1 me-2 d-block d-md-none" id="menu_toggle"><i class="fa-solid fa-bars"></i></span>
            <h3 class="mb-0">Mediclaim  View</h3>
            @*<span class="subTitle">(To be filled by hospital only)</span>*@
        </div>
        <p class="mb-0 col-6 top_link_Section">
            <a href="@Url.Action("Index", "Disbursment",new { Area="Mediclaim"})">Dashboard </a><span class="px-1">/</span> <a href="@Url.Action("ProcessingDashboard", "Disbursment",new { Area="Mediclaim"})">Processing Dashboard </a><span class="px-1"> /</span><span>Claim Approval View</span>
        </p>
    </div>
    <div class="card border-card mt-0">
        <input type="hidden" id="hdnAproveAmount" value="@Model.Cashless.ApprovedAmountInDecimal" />
        <div class="card-body">
            <div class="position-relative">
                <div id="material-tabs">
                    <a id="tab1-tab" href="#tab1" class="active">Voucher Detail</a>
                    <a id="tab2-tab" href="#tab2">Raised Claim Application</a>
                    <span class="yellow-bar"></span>
                </div>
            </div>
            <div class="tab-content view_container_bg ">
                <div id="tab1">
                    <div class="row">
                        <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                            <label class="form-label d-block mb-1">Claim No.</label>
                            <p class="View_text">@Model.Voucher.ClaimId</p>
                        </div>
                        <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                            <label class="form-label d-block mb-1">Claim Type</label>
                            <p class="View_text">Cashless</p>
                        </div>
                        <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                            <label class="form-label d-block mb-1">PPO Number</label>
                            <p class="View_text">@Model.Cashless.PPONumber</p>
                        </div>
                        <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                            <label class="form-label d-block mb-1">Medical Section Page Number </label>
                            <p class="View_text">@Model.Voucher.PageNo</p>
                        </div>

                        <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                            <label class="form-label d-block mb-1">Pay To shri / smt</label>
                            <p class="View_text">@Model.Voucher.PayTo</p>
                        </div>
                        <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3view_text_border">
                            <label class="form-label d-block mb-1">Approved Amount</label>
                            <p class="View_text">@Model.Voucher.ApprovedAmount.ToString("F")</p>
                        </div>
                        <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                            <label class="form-label d-block mb-1">In Words</label>
                            <p class="View_text">@Model.Voucher.AmountInWords</p>
                        </div>
                        <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                            <label class="form-label d-block mb-1">Account Number</label>
                            <p class="View_text">@Model.Voucher.AccountNumber</p>
                        </div>
                        <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 mb-3view_text_border">
                            <label class="form-label d-block mb-1">Bank name</label>
                            <p class="View_text">@Model.Voucher.BankName</p>
                        </div>
                        <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                            <label class="form-label d-block mb-1">Branch Name</label>
                            <p class="View_text">@Model.Voucher.BankBranch</p>
                        </div>
                        <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                            <label class="form-label d-block mb-1">BIC Code</label>
                            <p class="View_text">@Model.Voucher.BICCode</p>
                        </div>

                        <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                            <label class="form-label d-block mb-1">IFSC code </label>
                            <p class="View_text">@Model.Voucher.IFSCCode</p>
                        </div>



                    </div>
                    <div class="row mb-3 submit_area submit_area_strip mt-5 mx-0">

                        <div class="col-sm-12">
                            <button class="btn btn-primary" id="printMe">
                                <i class="fa fa-print mr-2"
                                   aria-hidden="true"></i>Print Application
                            </button>
                        </div>
                    </div>
                </div>
                <div id="tab2">
                    <div class="border-card  mt-0 px-0">
                        <h4 class="form_title shadow-2-strong ">
                            Detail Of Hospital
                        </h4>
                        <div class="row">
                            <div class="col-md-6 col-lg-6 col-xl-6 mb-3 view_text_border">
                                <label class="form-label d-block mb-1" for="name">
                                    Name Of Hospital
                                </label>
                                <p class="View_text">@Model.Cashless.NameOfHospital</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">
                                    Hospital Id
                                </label>
                                <p class="View_text">@Model.Cashless.HospitalId</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Phone Number</label>
                                <p class="View_text">@Model.Cashless.HospitalPhoneNumber</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Email Id</label>
                                <p class="View_text">@Model.Cashless.EmailId</p>
                            </div>
                            <div class="col-sm-12 col-md-9 col-lg-9 col-xl-9 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Address</label>
                                <p class="View_text">@Model.Cashless.HospitalAddress</p>
                            </div>
                        </div>
                    </div>
                    <div class="border-card  mt-0 px-0">

                        <h4 class="form_title">
                            Card Holder Detail
                        </h4>
                        <div class="row">
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Name of medical card holder</label>
                                <p class="View_text">@Model.Cashless.NameOfCardHolder</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Medical Card number</label>
                                <p class="View_text">@Model.Cashless.MedicalCardNumber</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Card Category</label>
                                <p class="View_text">@Model.Cashless.CardCategory</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Medical Section Page Number</label>
                                <p class="View_text">@Model.Cashless.MedicalSectionPageNumber</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Email Id</label>
                                <p class="View_text">@Model.Cashless.PatientEmailId</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3 view_text_border">
                                <label class="form-label d-block mb-1" for="name">PPO Number</label>
                                <p class="View_text">@Model.Cashless.PPONumber</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Date Of Birth</label>
                                <p class="View_text">@Model.Cashless.DateOfBirth.ToString("dd-MM-yyyy")</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3view_text_border">
                                <label class="form-label d-block mb-1">Age</label>
                                <p class="View_text">@Model.Cashless.Age</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Mobile Number</label>
                                <p class="View_text">@Model.Cashless.PatientPhoneNumber</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-6 col-xl-6  mb-3 view_text_border">
                                <label class="form-label d-block mb-1" for="name">Organization</label>
                                <p class="View_text">@Model.Cashless.Organization</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3 view_text_border">
                                <label class="form-label d-block mb-1" for="name">Department</label>
                                <p class="View_text">@Model.Cashless.Department</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3 view_text_border">
                                <label class="form-label d-block mb-1" for="name">Designation</label>
                                <p class="View_text">@Model.Cashless.Designation</p>
                            </div>

                            <div class="col-sm-6 col-md-6 col-lg-6 col-xl-9 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Address</label>
                                <p class="View_text">@Model.Cashless.PatientAddress</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Date Of Retirement</label>
                                <p class="View_text">@Model.Cashless.DateOfRetirement.ToString("dd-MM-yyyy")</p>
                            </div>
                        </div>
                        <h4 class="form_title mt-3 shadow-2-strong">
                            Detail Of Patients
                        </h4>
                        <div class="row">
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3 view_text_border">
                                <label class="form-label d-block mb-1" for="name">Name Of Patient</label>
                                <p class="View_text">@Model.Cashless.NameOfPatient</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">relationship with  retiree   </label>
                                <p class="View_text">@Model.Cashless.RelationWithRetire</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 mb-3view_text_border">
                                <label class="form-label d-block mb-1">Gender</label>
                                <p class="View_text">@Model.Cashless.Gender</p>
                            </div>




                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Date Of Birth</label>
                                <p class="View_text">@Model.Cashless.DependantDOB.ToString("dd-MM-yyyy")</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3view_text_border">
                                <label class="form-label d-block mb-1">Age</label>
                                <p class="View_text">@Model.Cashless.DependantAge</p>
                            </div>

                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Date of Admission </label>
                                <p class="View_text">@Model.Cashless.DateOfAdmission.ToString("dd-MM-yyyy")</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Admission Number</label>
                                <p class="View_text">@Model.Cashless.AdmissionNumber</p>
                            </div>

                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Illness Type</label>
                                <p class="View_text">@Model.Cashless.CaseType</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-6 col-xl-6 mb-3 view_text_border">
                                <label class="form-label d-block mb-1"> Type Of treatment </label>
                                <p class="View_text">@Model.Cashless.TypeOfTreatment</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Claim Amount</label>
                                <p class="View_text">@Model.Cashless.Amount</p>
                            </div>

                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3 view_text_border">
                                <label class="form-label d-block mb-1"> Date of Discharge / Date of Death </label>
                                <p class="View_text">@Model.Cashless.DateOfDischargeOrDeath.ToString("dd-MM-yyy")</p>
                            </div>


                        </div>

                    </div>

                    <div class=" border-card border-top-0 px-0">
                        <h4 class="form_title mt-3 shadow-2-strong">
                            Claim Document Upload
                        </h4>
                        <div class="row">

                            <div class="col-sm-12">
                                <table id="example1" class="table table-bordered view_table">
                                    <thead>
                                        <tr>
                                            <th width="7%" class="text-center">Sr. No.</th>
                                            <th>Document Type</th>
                                            <th width="15%" class="text-center">Attach file </th>

                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (Model.Cashless.Documents.Count() > 0)
                                        {

                                            var _index = 1;
                            @foreach (var item in Model.Cashless.Documents)
                            {
                <tr>
                    <td>
                        @_index
                    </td>
                    <td>
                        @item.DocumentFor
                    </td>
                    <td class="text-center">
                        <a href="/@item.DocumentPath" class="pdfIcons" target="_blank"><i class="fa fa-file-pdf" aria-hidden="true"></i></a>
                    </td>
                </tr>_index = _index + 1;
                                }}
                                    </tbody>
                                </table>

                            </div>
                        </div>
                    </div>
                    <div class=" border-card  border-top-0 px-0">
                        <h4 class="form_title mt-3 ">
                            Bank Details
                        </h4>
                        <div class="row ">

                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Account Holder Name</label>
                                <p class="View_text">@Model.Cashless.AccountHolderName</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Account Number</label>
                                <p class="View_text">@Model.Cashless.AccountNumber</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Bank Name</label>
                                <p class="View_text">@Model.Cashless.BankName</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Branch Name</label>
                                <p class="View_text">@Model.Cashless.BranchName</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">BIC Code</label>
                                <p class="View_text">@Model.Cashless.BICCode</p>
                            </div>

                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">IFSC code </label>
                                <p class="View_text">@Model.Cashless.IFSCNumber</p>
                            </div>

                        </div>
                    </div>
                    <div class=" border-card border-top-0 px-0">
                        <div class="card-body">
                            <h4 class="form_title shadow-2-strong text-start">
                                Claim Approval status
                            </h4>
                            <div class="row">
                                <div class="col-sm-4 mb-3 col-md-3 col-lg-2 col-xl-2">
                                    <label class="form-label d-block mb-1 text-left">
                                        Approve Amount<span class="text-danger req_text">*</span>
                                    </label>
                                    <input type="text" id="txtApproveAmount" disabled class="form-control form-control-sm" value="@Model.Cashless.ApprovedAmountInDecimal">

                                </div>
                                <div class="col-sm-6 col-md-6 mb-6 col-lg-6 col-xl-6 ">
                                    <label class="form-label d-block mb-1 text-left">
                                        In Words<span class="text-danger req_text">*</span>
                                    </label>
                                    <input type="text" id="txtApproveAmountInWords" disabled class="form-control form-control-sm" placeholder="Approve Amount in Words">

                                </div>
                            </div>

                            <h4 class="form_title shadow-2-strong text-start">
                                Dealing assistant remark
                            </h4>
                            <div class="row ">
                                <div class="col-sm-8 mb-3 pr-0">
                                    <label class="form-label d-block mb-1 text-left">
                                        Dealing assistant Remarks<span class="text-danger req_text">*</span>
                                    </label>
                                    <textarea type="text" id="txtDealingAssistantRemark" class="form-control form-control-sm" readonly disabled>@Model.Cashless.DealingAssistanceRemark</textarea>
                                </div>
                            </div>
                            <h4 class="form_title shadow-2-strong text-start">
                                Aso (Assistant Specialist Officer )
                            </h4>
                            <div class="row">
                                <div class="col-sm-8 mb-3 pr-0">
                                    <label class="form-label d-block mb-1 text-left">
                                        ASO Remarks<span class="text-danger req_text">*</span>
                                    </label>
                                    <textarea type="text" id="txtASORemark" class="form-control form-control-sm" readonly disabled>@Model.Cashless.ASORemark</textarea>
                                </div>
                            </div>
                            <h4 class="form_title shadow-2-strong text-start">
                                so ( Specialist Officer )
                            </h4>
                            <div class="row">
                                <div class="col-sm-8 mb-3 pr-0">
                                    <label class="form-label d-block mb-1 text-left">
                                        SO Remarks<span class="text-danger req_text">*</span>
                                    </label>
                                    <textarea type="text" id="txtSORemark" class="form-control form-control-sm" readonly disabled>@Model.Cashless.SORemark</textarea>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row mb-3 submit_area submit_area_strip mt-5 mx-0">

                        <div class="col-sm-12">
                            <button class="btn btn-primary" id="printMeApplication">
                                <i class="fa fa-print mr-2"
                                   aria-hidden="true"></i>Print Application
                            </button>
                        </div>
                    </div>  
                
                </div>
           
            </div>
        </div>
    </div>
 
</div>


