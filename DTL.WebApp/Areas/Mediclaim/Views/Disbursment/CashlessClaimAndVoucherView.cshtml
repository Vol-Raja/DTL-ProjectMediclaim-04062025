<div id="print_previewApplication">
    <div class=" view_container_bg mt-0">
        <div class="card-body">
            <h4 class="print_title">
                Cashless Mediclaim
            </h4>
            <table class="table table-bordered w-100">
                <tbody>
                    <tr>
                        <th scope="row">Name Of Hospital</th>
                        <td scope="row">@Model.Cashless.NameOfHospital</td>
                        <th scope="row">Hospital Id</th>
                        <td scope="row">@Model.Cashless.HospitalId</td>

                    </tr>
                    <tr>
                        <th scope="row">Phone Number</th>
                        <td scope="row">@Model.Cashless.HospitalPhoneNumber</td>
                        <th scope="row">Email Id</th>
                        <td scope="row">@Model.Cashless.EmailId</td>
                    </tr>
                    <tr>
                        <th scope="row">Address</th>
                        <td scope="row" colspan="3">@Model.Cashless.HospitalAddress</td>
                    </tr>
                    <tr>
                        <td scope="row" colspan="4" class="text-center h4">Details Of Patient</td>
                    </tr>
                    <tr>
                        <th scope="row" colspan="2">Name of Patient</th>
                        <td scope="row" colspan="2">@Model.Cashless.NameOfPatient</td>
                    </tr>
                    <tr>
                        <th scope="row">PPO Number</th>
                        <td scope="row">@Model.Cashless.PPONumber</td>
                        <th scope="row">Gender</th>
