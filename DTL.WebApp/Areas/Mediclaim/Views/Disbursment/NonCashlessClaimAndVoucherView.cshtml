<!--Print Preview-->
<div id="print_preview">
    <div class=" view_container_bg mt-0">
        <div class="card-body">
            <h4 class="print_title">
                Mediclaim Voucher
            </h4>
            @if (Model.Voucher != null)
            {
                <table class="table table-bordered">
                    <tbody>
