@using DTL.Model.Models.Mediclaim
@model ClaimAndVoucherDetailModel
@{ ViewData["Title"] = "NonCashless";
    Layout = "~/Views/Shared/_Layout.cshtml"; }
<style>
    .top_print_btn {
        position: absolute;
        right: 20px;
        top: 30px;
    }

    .view_container_bg {
        background: rgb(0 43 255 / 4%);
    }

    .view_table tr td {
        color: #005ccf;
    }
</style>
<!--New UI-->

<div class="container-fluid view_form">
    <div class="row header_section mx-0 px-0">
        <div class="col-6 position-relative ps-0">
            <span class="btn btn-light float-start mt-1 me-2 d-block d-md-none" id="menu_toggle"><i class="fa-solid fa-bars"></i></span>
            <h3 class="mb-0">Mediclaim  View</h3>
            @*<span class="subTitle">(To be filled by hospital only)</span>*@
        </div>
        <p class="mb-0 col-6 top_link_Section">
            <span class=""><a href="@Url.Action("Index","Home",new { Area="" })">Dashboard</a></span><span class="px-1">/</span><span><a href="@Url.Action("NonCashlessProcessingList","Disbursment",new { Area="Mediclaim" })">Non-Cashless</a></span><span class="px-1">/</span><span>Claim & Voucher View</span>
        </p>
    </div>
    <div class="card border-card mt-0">
        <div class="card-body">
            <div class="position-relative">
                <div id="material-tabs">
                    <a id="tab1-tab" href="#tab1" class="active">Voucher Detail</a>
                    <a id="tab2-tab" href="#tab2">Raised Claim Application</a>
                    <span class="yellow-bar"></span>
                </div>
            </div>
            <div class="tab-content view_container_bg ">
                <div id="tab1">
                    @if (Model.Voucher != null)
                    {
                        <div class="row">
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Claim No.</label>
                                <p class="View_text">@Model.Voucher.ClaimId</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Claim Type</label>
                                <p class="View_text">Non Cashless</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">PPO Number</label>
                                <p class="View_text">@Model.NonCashless.PPONumber</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Medical Section Page number </label>
                                <p class="View_text">@Model.Voucher.PageNo</p>
                            </div>

                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Pay To shri / smt</label>
                                <p class="View_text">@Model.Voucher.PayTo</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Approved Amount</label>
                                <p class="View_text">@Model.Voucher.ApprovedAmountInDecimal</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-6 col-xl-6 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">In Words</label>
                                <p class="View_text">@Model.Voucher.AmountInWords</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Account Number</label>
                                <p class="View_text">@Model.Voucher.AccountNumber</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Bank name</label>
                                <p class="View_text">@Model.Voucher.BankName</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">Branch Name</label>
                                <p class="View_text">@Model.Voucher.BankBranch</p>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">BIC Code</label>
                                <p class="View_text">@Model.Voucher.BICCode</p>
                            </div>

                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                <label class="form-label d-block mb-1">IFSC code </label>
                                <p class="View_text">@Model.Voucher.IFSCCode</p>
                            </div>


                        </div>
                        <div class="row mb-3 submit_area submit_area_strip mt-5 mx-0">
                            <div class="col-sm-12">
                                <button class="btn btn-primary" id="printMe">
                                    <i class="fa fa-print mr-2"
                                       aria-hidden="true"></i>Print voucher
                                </button>
                            </div>
                        </div> }
                    else
                    {
                        <div class="col-sm-6 col-md-6 col-lg-6 col-xl-6 mb-3 view_text_border">
                            <label class="form-label d-block mb-1">Voucher is not generated for this claim</label>
                        </div>}
                    @*<div class="row mb-3 submit_area submit_area_strip mt-5 mx-0">
                            <div class="col-sm-12">
                                <button class="btn btn-primary" id="printMe">

                                    <i class="fa fa-print mr-2"
                                       aria-hidden="true"></i>Print voucher
                                </button>
                            </div>
                        </div>*@
                </div>
                <div id="tab2">
                    @if (Model.NonCashless != null)
                    {
                        <input type="hidden" id="hdnAproveAmount" value="@Model.NonCashless.ApprovedAmountInDecimal" />
                        <div class="border-card mt-0  ">
                            <h4 class="form_title">
                                Personal details
                            </h4>
                            <div class="row">
                                <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                    <label class="form-label d-block mb-1">Employee number</label>
                                    <p class="View_text">@Model.NonCashless.EmployeeNumber</p>
                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                    <label class="form-label d-block mb-1">PPO number</label>
                                    <p class="View_text">@Model.NonCashless.PPONumber</p>
                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                    <label class="form-label d-block mb-1">Medical Section Page Number </label>
                                    <p class="View_text">@Model.NonCashless.MedicalSectionPageNumber</p>
                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
