    <script type="text/javascript">
        var numberToWord = new NumberToWord();
        $('#lblAmountInWords').text(numberToWord.ToWord($('#hdnAproveAmount').val()));
        $('#lblAmountInWordsPV').text(numberToWord.ToWord($('#hdnAproveAmount').val()));
        $('#txtApproveAmountInWords').val(numberToWord.ToWord($('#hdnAproveAmount').val()));
    </script>

    <script type="text/javascript">
        $('#print_preview_nonashless_raise').hide();

        $('#printMe').click(function () {
            $('#print_preview_nonashless_raise').removeClass('show');
            $('#print_preview').addClass('show');
            window.print();
        });

        $('#printMeNonCashlessRaise').click(function () {
            $('#print_preview').removeClass('show');
            $('#print_preview_nonashless_raise').addClass('show');
            window.print();
        });

    </script>
