                                    <p class="View_text">@Model.NonCashless.Age</p>
                                </div>

                                <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                    <label class="form-label d-block mb-1">Mobile Number</label>
                                    <p class="View_text">@Model.NonCashless.MobileNumber</p>
                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                    <label class="form-label d-block mb-1">Email Id</label>
                                    <p class="View_text">@Model.NonCashless.EmailId</p>
                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                    <label class="form-label d-block mb-1">Card Category</label>

                                    <p class="View_text">@Model.NonCashless.CardCategory</p>
                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-6 col-xl-3 mb-3 view_text_border">
                                    <label class="form-label d-block mb-1">Dependent/Self</label>
                                    <p class="View_text">@Model.NonCashless.ClaimFor</p>
                                </div>
                                <div class="col-sm-6 col-md-6 col-lg-6 col-xl-3 mb-3 view_text_border">
                                    <label class="form-label d-block mb-1">Name of Patient</label>
                                    <p class="View_text">@Model.NonCashless.PatientName</p>
                                </div>

                                <div class="col-sm-6 col-md-6 col-lg-6 col-xl-3 mb-3 view_text_border">
                                    <label class="form-label d-block mb-1">Claim Type Serial No</label>
                                    <p class="View_text">@Model.NonCashless.ClaimNumber</p>
                                </div>

                                <div class="col-sm-12 col-md-9 col-lg-9 col-xl-9 mb-3 view_text_border">
                                    <label class="form-label d-block mb-1">Address</label>
                                    <p class="View_text">@Model.NonCashless.Address</p>
                                </div>
                            </div>
                        </div>
                        @if (Model.NonCashless.Dependent != null && Model.NonCashless.ClaimFor.ToLower() == "dependent")
                        {
                            <div class="border-card  border-top-0 ">
                                <h4 class="form_title">
                                    Dependent Information
                                </h4>
                                <div class="row ">

                                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                        <label class="form-label d-block mb-1"> Name</label>
                                        <p class="View_text">@Model.NonCashless.Dependent.Name</p>
                                    </div>
                                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                        <label class="form-label d-block mb-1">Date Of Birth</label>
                                        <p class="View_text">@Model.NonCashless.Dependent.DateOfBirth.ToString("dd-MM-yyyy")</p>
                                    </div>
                                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                        <label class="form-label d-block mb-1">Age</label>
                                        <p class="View_text">@Model.NonCashless.Dependent.Age</p>
                                    </div>
                                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3 view_text_border">
                                        <label class="form-label d-block mb-1">
                                            relationship with retiree
                                        </label>
                                        <p class="View_text">@Model.NonCashless.Dependent.RelationWithRetire</p>
                                    </div>
                                </div>
                            </div>}

                <div class="border-card  border-top-0 ">
                    <h4 class="form_title">
                        Detail of Hospitalization
                    </h4>

                    @if (Model.NonCashless.ClaimType.ToLower() == "opd/dispensary")@*OPD / Dispensary*@ {
                        <div class="row mb-3">
                            <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12 mb-12 view_text_border">
                                <label class="form-label d-block mb-1">Type of claim</label>
                                <p class="View_text">OPD/Dispensary</p>
                            </div>
                        </div>}
                    @if (Model.NonCashless.ClaimType.ToLower() == "cancer/dialysis")
                    {
                        <div class="row mb-3">
                            <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12 mb-12 view_text_border">
                                <label class="form-label d-block mb-1">Type of claim</label>
                                <p class="View_text">Cancer/Dialysis</p>
                            </div>
                        </div>}
                    @if (Model.NonCashless.ClaimType.ToLower() == "ipd/noncashless")
                    {
                        <div class="row mb-3">
                            <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12 mb-12 view_text_border">
                                <label class="form-label d-block mb-1">Type of claim</label>
                                <p class="View_text">IPD/NonCashless</p>
                            </div>
                        </div>

                    }

                    <div class="row ">
                        <div class="col-sm-12 mt-4">
                            <table id="tblCND" class="table table-bordered">
                                <thead>
                                    <tr>
                                        <th>Date</th>
                                        <th>Name of Hospital</th>
                                        <th>Medicine Amt</th>
                                        <th>Consultation Amt</th>
                                        <th>Investigation Amt</th>
                                        <th>Other Expenses Amt</th>
                                        <th class="text-right">Total Amount</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @{
                                        decimal totalAmountSum = 0;
                                    }

                                    @foreach (var item in Model.NonCashless.OPDCNDList)
                                    {
                                        totalAmountSum += item.TotalAmount;

                                        <tr>
                                            <td>@item.OPDCNDDate.ToString("dd-MM-yyyy")</td>
                                            <td>@item.HospitalName</td>
                                            <td class="text-right">@Math.Round(item.MedicineAmount, 2)<span class="text-danger req_text"> ₹</span></td>
                                            <td class="text-right">@Math.Round(item.ConsultationAmount, 2)<span class="text-danger req_text"> ₹</span></td>
                                            <td class="text-right">@Math.Round(item.InvestigationAmount, 2)<span class="text-danger req_text"> ₹</span></td>
                                            <td class="text-right">@Math.Round(item.OtherAmount, 2)<span class="text-danger req_text"> ₹</span></td>
                                            <td class="text-right">@Math.Round(item.TotalAmount, 2)<span class="text-danger req_text"> ₹</span></td>
                                        </tr>
                                    }

                                    <!-- Grand Total Row -->
                                    <tr style="font-weight:bold; background-color:#f8f9fa;">
                                        <td colspan="6" class="text-right">Grand Total:</td>
                                        <td class="text-right">@Math.Round(totalAmountSum, 2)<span class="text-danger req_text"> ₹</span></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>


