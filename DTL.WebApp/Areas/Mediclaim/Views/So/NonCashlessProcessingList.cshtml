@{ ViewData["Title"] = "NonCashless Processing";
    Layout = "~/Views/Shared/_Layout.cshtml"; }
<div class="container-fluid">
    <div class="row header_section mx-0 px-0">
        <div class="col-6 position-relative ps-0">
            <span class="btn btn-light float-start mt-1 me-2 d-block d-md-none" id="menu_toggle">
                <i class="fa-solid fa-bars"></i>
            </span>
            <h3 class="mb-0">Processing mediclaim (Non cashless ) List</h3>
        </div>
        <p class="mb-0 col-6 top_link_Section">
            <span class=""> Dashboard </span><span class="px-1">/</span><span>Processing mediclaim (Non cashless )</span>

        </p>
    </div>

    <div class="card border-card mt-0">
        <div class="card-body pt-0 ps-0 pr-2">
            <div class="my-4 border_bottom pb-4 row">

                <div class="col-sm-6 col-md-3 col-lg-3 col-xl-2 ">
                    <label class="form-label d-block mb-1">
                        Date<span class="text-danger req_text">*</span>
                    </label>
                    <input type="date" class="form-control form-control-sm"
                           placeholder=" Date" />
                </div>
                <div class="col-sm-6 col-md-3 col-lg-3 col-xl-2 ">
                    <label class="form-label d-block mb-1">
                        Claim No.<span class="text-danger req_text">*</span>
                    </label>
                    <input type="Number" class="form-control form-control-sm"
                           placeholder="Claim No." />
                </div>
                <div class="col-sm-6 col-md-2 col-lg-2 col-xl-2">
                    <label class="form-label d-block mb-2">Card Category</label>
                    <select name="claimType" id="claimType" class="form-select ">
                        <option value="">Select</option>
                        <option value="0">Private</option>
                        <option value="1">Semi Private</option>
                    </select>
                </div>
                <div class="col-sm-6 col-md-2 col-lg-2 col-xl-2">
                    <label class="form-label d-block mb-2">Organization</label>
                    <select name="claimType" id="claimType" class="form-select ">
                        <option value="0">Select</option>
                        <option value="1">Organization</option>

                    </select>
                </div>
                <div class="col-sm-6 col-md-3 col-lg-3 col-xl-3  position-relative">
                    <button class="btn btn-success  position-absolute bottom-0"><i class="fa fa-search"></i></button>
                    <button class="btn btn-danger position-absolute bottom-0 clear_btn"><i class="fa fa-times"></i></button>
                </div>
            </div>

            <table id="example1" class="table table-bordered">
                <thead>
                    <tr>
                        <th width="7%" class="text-center">Sr. No.</th>
                        <th>Date</th>
                        <th>Claim No</th>
                        <th>PPO No.</th>
                        <th>Card Category</th>
                        <th>Type Of Claim</th>
                        <th>Organization</th>
                        <th>Patient Name</th>
                        <th>Claim Status</th>
                        <th>Change Status</th>
                        <th>Remark</th>
                        <th>Physical Submit</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td class="text-center">1</td>
                        <td>25/10/2021</td>
                        <td>9090</td>
                        <td>PPO01</td>
                        <td>Semi private</td>
                        <td>OPD</td>
                        <td>Organization</td>
                        <td>Rahul</td>
                        <td class="text-center">
                            <span class="bg-danger Status_text">Reject</span>
                        </td>
                        <td>
                            <select name="" id="claimType" class="form-select table_select">
                                <option value="">Select</option>
                                <option value="">Forword To ASO</option>
                                <option value="">Forword To SO</option>
                                <option value="">Forword To AM/DM</option>
                            </select>
                        </td>
                        <td class="para">Neque porro quisquam est qui dolorem</td>
                        <td class="text-center"><input type="checkbox" id="Physical" name="vehicle1" checked></td>
                        <td class="d-flex border-bottom-0">
                            <a href="@Url.Action("NonCashlessAppovedReject","So", new { Area="Mediclaim" })" class="btn btn-primary btn-sm mr-2 btn_small"><i class="fa fa-eye" aria-hidden="true"></i></a>

                            <!-- Modal -->
                        </td>
                    </tr>
                    <tr>
                        <td class="text-center">1</td>
                        <td>25/10/2021</td>
                        <td>9090</td>

                        <td>PPO01</td>
                        <td>Semi private</td>
                        <td>IPD</td>

                        <td>Organization</td>
                        <td>Rahul</td>
                        <td class="text-center">
                            <span class="bg-success Status_text">Approved</span>

                        </td>
                        <td>
                            <select name="" id="claimType" class="form-select table_select">
                                <option value="">Select</option>
                                <option value="">Forword To ASO</option>
                                <option value="">Forword To SO</option>
                                <option value="">Forword To AM/DM</option>
                            </select>
                        </td>
                        <td class="para">Neque porro quisquam est qui dolorem</td>
                        <td class="text-center"><input type="checkbox" id="Physical" name="vehicle1"></td>
                        <td class="d-flex border-bottom-0">
                            <a href="@Url.Action("NonCashlessAppovedReject","So", new { Area="Mediclaim" })" class="btn btn-primary btn-sm mr-2 btn_small"><i class="fa fa-eye" aria-hidden="true"></i></a>

                            <!-- Modal -->
                        </td>
                    </tr>
                    <tr>
                        <td class="text-center">1</td>
                        <td>25/10/2021</td>
                        <td>9090</td>

                        <td>PPO01</td>
                        <td>Semi private</td>
                        <td width="10%">Cancer</td>
                        <td>Organization</td>
                        <td>Rahul</td>
                        <td class="text-center">
                            <span class="bg-warning Status_text">Pending</span>
                        </td>
                        <td>
                            <select name="" id="claimType" class="form-select table_select">
                                <option value="">Select</option>
                                <option value="">Forword To ASO</option>
                                <option value="">Forword To SO</option>
                                <option value="">Forword To AM/DM</option>
                            </select>
                        </td>
                        <td class="para">Neque porro quisquam est qui dolorem</td>
                        <td class="text-center"><input type="checkbox" id="Physical" name="vehicle1"></td>
                        <td class="d-flex border-bottom-0">
                            <a href="@Url.Action("NonCashlessAppovedReject","So", new { Area="Mediclaim" })" class="btn btn-primary btn-sm mr-2 btn_small"><i class="fa fa-eye" aria-hidden="true"></i></a>

                            <!-- Modal -->
                        </td>
                    </tr>
                </tbody>
            </table>

        </div>
    </div>
</div><!-- col -->
<!-- /.content-wrapper -->
@section NonCashlessClaims{
<script type="text/javascript" src="~/js/Mediclaim/Processing/noncashless.js"></script>
<script>
        (function ($) {

            $('#example1').DataTable({
                "ordering": true,
                "sScrollX": "100%",
                "sScrollXInner": "100%",
                "bAutoWidth": false,
                "bJQueryUI": true,
                "bRetrieve": true,
                "bAutoWidth": true
            });
        })(jQuery);

</script>
