@using DTL.Model.Models
@model IEnumerable<MasterDocumentModel>
@{ ViewData["Title"] = "NonCashless";
                Layout = "~/Views/Shared/_Layout.cshtml"; }


<div class="container-fluid" style="margin:0px">
    <div class="row header_section mx-0 px-0">
        <div class="col-6 position-relative ps-0">
            <span class="btn btn-light float-start mt-1 me-2 d-block d-md-none" id="menu_toggle"><i class="fa-solid fa-bars"></i></span>
            <h3 class="mb-0">Non-Cashless Mediclaim Form </h3>
        </div>
        <p class="mb-0 col-6 top_link_Section">
            <a href="@Url.Action("Index","OPD",new { Area="Mediclaim" })">Dashboard</a><span class="px-1">/</span>
            @*<a href="/Mediclaim/OPD">Mediclaim</a><span class="px-1">/</span>*@
            <a href="/Mediclaim/NonCashless">Non-Cashless Claims</a><span class="px-1">/</span>
            <span>Raise</span>
        </p>
    </div>
    <!-- /.content-header -->
    <!-- Main content -->
    <div class="card border-card mt-0">

        <div class="card-body">
            <form id="nonCashlessForm" class="g-3 mt-2 needs-validation" novalidate="">
                <input type="hidden" id="hdnMediClaimId" />
                <h4 class="form_title shadow-2-strong ">
                    Personal Details
                </h4>
                <div class="row">

                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-2  mb-3">
                        <label class="form-label d-block mb-1" for="name">PPO No<span class="text-danger req_text">*</span></label>
                        <input type="text" id="txtPPONumber" class="form-control form-control-sm " placeholder="PPO No">
                        <div class="invalid-feedback">
                            Please Enter PPO Number
                        </div>
                    </div>


                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-2  mb-3">
                        <label class="form-label d-block mb-1" for="name">Employee No.<span class="text-danger req_text">*</span></label>
                        <input type="text" id="txtEmployeeNumber" class="form-control form-control-sm " placeholder="Employee No">
                        <div class="invalid-feedback ">
                            Please Enter Employee Number
                        </div>
                    </div>

                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3">
                        <label class="form-label d-block mb-1" for="name">Medical Section page Number<span class="text-danger req_text"></span></label>
                        <input type="text" id="txtMedicalSectionPageNumber" class="form-control form-control-sm " placeholder="Medical section page number">
                        <div class="invalid-feedback ">
                            Please Enter Medical Section Page Number
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-5 mb-3">
                        <label class="form-label d-block mb-1" for="name">Name of Pensioner<span class="text-danger req_text">*</span></label>
                        <input type="text" id="txtMedicalCardHolderName" class="form-control form-control-sm " placeholder="Name of Pensioner" onkeypress="return  lettersOnly(event)">
                        <div class="invalid-feedback ">
                            Please Enter Name of Pensioner
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-4  mb-3">
                        <label class="form-label d-block mb-1" for="name">Medical card number<span class="text-danger req_text"></span></label>
                        <input type="text" id="txtMedicalCardNo" class="form-control form-control-sm " placeholder="Medical Card Number">
                        <div class="invalid-feedback ">
                            Please Enter Medical Card Number
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">Gender</label>
                        <div class="d-flex mt-3 gender-radio">
                            <div class="form-check me-3">
                                <input class="form-check-input" type="radio" name="gender" id="rdMale" value="Male" checked="checked">
                                <label class="form-check-label" for="male">
                                    Male
                                </label>
                            </div>
                            <div class="form-check me-3">
                                <input class="form-check-input" type="radio" name="gender" id="rdFemale" value="Female">
                                <label class="form-check-label" for="female">
                                    Female
                                </label>
                            </div>
                            <div class="form-check">
                                <input class="form-check-input" type="radio" name="gender" id="rdOther" value="Other">
                                <label class="form-check-label" for="other">
                                    Other
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-5 mb-3">
                        <label class="form-label d-block mb-1" for="name">Organization<span class="text-danger req_text">*</span></label>
                        @*<select name="" id="selOrganization" class="form-select ">
                <option value="">Select</option>
            </select>*@
                        @Html.DropDownList("Organization", (IEnumerable<SelectListItem>)ViewBag.DTLOfficeList, "Select", new { id = "selOrganization", @class = "form-select" })
                        <div class="invalid-feedback ">
                            Please Enter Organization
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-6  mb-3">
                        <label class="form-label d-block mb-1" for="name">Designation<span class="text-danger req_text"></span></label>
                        <input type="text" id="txtDesignation" class="form-control form-control-sm " placeholder="Designation">
                        <div class="invalid-feedback ">
                            Please Enter Designation
                        </div>
                    </div>


                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-2 mb-3">
                        <label class="form-label d-block mb-1">Date Of Birth<span class="text-danger req_text">*</span></label>
                        <input type="Date" id="txtDateOfBirth" class="form-control form-control-sm" placeholder="Date Of Birth">
                        <div class="invalid-feedback ">
                            Please Enter Date of Birth
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-1 mb-3">
                        <label class="form-label d-block mb-1">Age<span class="text-danger req_text">*</span></label>
                        <input type="number" id="txtAge" class="form-control form-control-sm" placeholder="Age" readonly>
                        <div class="invalid-feedback ">
                            Please Enter Age
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3">
                        <label class="form-label d-block mb-1">Mobile Number<span class="text-danger req_text">*</span></label>
                        <input type="text" id="txtMobileNumber" maxlength="10" class="form-control" placeholder="Mobile Number" required onkeypress="return  isNumberKey(event)">
                        <div class="invalid-feedback " id="mobileNoNull">
                            Please Enter Valid Mobile Number
                        </div>

                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-6  mb-3">
                        <label class="form-label d-block mb-1">Email id<span class="text-danger req_text"></span></label>
                        <input type="email" id="txtEmailId" class="form-control form-control-sm" placeholder="Email id">
                        <div class="invalid-feedback ">
                            Please Enter Valid Email Id
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-6 mb-3">
                        <label class="form-label d-block mb-1">Card Category<span class="text-danger req_text">*</span></label>
                        <select name="" id="selCardCategory" class="form-select ">
                            <option value="">Select</option>
                            <option>General</option>
                            <option>Private</option>
                            <option>Semi Private</option>
                        </select>
                        <div class="invalid-feedback ">
                            Please Enter Card Category
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-6 mb-3">
                        <label class="form-label d-block mb-1">Address<span class="text-danger req_text">*</span></label>
                        <textarea class="form-control form-control-sm" id="txtAddress" placeholder="Address"></textarea>
                        <div class="invalid-feedback ">
                            Please Enter Address
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3">
                        <label class="form-label d-block mb-1">Name of Patient<span class="text-danger req_text">*</span></label>
                        <input type="text" id="txtPatientName" class="form-control form-control-sm" placeholder="Name of Patient">
                        <div class="invalid-feedback ">
                            Please Enter Name of Patient
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1"> Dependent / Self</label>
                        <div class="d-flex mt-3 ds-radio">
                            <div class="form-check me-3">
                                <input class="form-check-input" type="radio" name="dependent_self" id="dependent" value="Dependent">
                                <label class="form-check-label" for="dependent">
                                    Dependent
                                </label>
                            </div>
                            <div class="form-check me-3">
                                <input class="form-check-input" type="radio" name="dependent_self" id="self" value="Self" checked="checked">
                                <label class="form-check-label" for="dependentSelf">
                                    Self
                                </label>
                            </div>

                        </div>
                    </div>
                    <div class="col-sm-12 showDependent" id="showMe" style="display: none;">

                        <h4 class="form_title shadow-2-strong ">
                            Dependent Information
                        </h4>
                        <div class="row">
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-4  mb-3">
                                <label class="form-label d-block mb-1">Name <span class="text-danger req_text">*</span></label>
                                <input type="text" id="txtDependentName" class="form-control form-control-sm" placeholder="Name">
                                <div class="invalid-feedback ">
                                    Please Enter Name of Patient
                                </div>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-2 mb-3">
                                <label class="form-label d-block mb-1">Date Of Birth<span class="text-danger req_text"></span></label>
                                <input type="Date" id="txtDependentDob" class="form-control form-control-sm" placeholder="Date Of Birth">
                                <div class="invalid-feedback ">
                                    Please Enter Date Of Birth
                                </div>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-1 mb-3">
                                <label class="form-label d-block mb-1">Age<span class="text-danger req_text">*</span></label>
                                <input type="number" id="txtDependentAge" class="form-control form-control-sm" placeholder="Age">
                                <div class="invalid-feedback ">
                                    Please Enter Age
                                </div>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-4 col-xl-5 mb-3">
                                <label class="form-label d-block mb-1">
                                    relationship with
                                    retiree<span class="text-danger req_text">*</span>
                                </label>
                                <select name="RelatioWithRetire" id="selRelatioWithRetire" class="form-select ">
                                    <option value="">Select</option>
                                    <option value="Self">Self</option>
                                    <option value="Spouse">Spouse</option>
                                    <option value="Parents">Parents</option>
                                    <option value="Child">Child</option>
                                </select>
                                <div class="invalid-feedback ">
                                    Please Select dependent relationship with
                                    retiree
                                </div>
                            </div>
                        </div>
                    </div>

                </div>

                <h4 class="form_title shadow-2-strong ">
                    Detail of Hospitalization
                </h4>

