                <div class="row mt-4" id="CD_div">

                    <!-- Claim Type -->
                    <div class="col-sm-6 col-md-4 col-lg-3 mb-3">
                        <label class="form-label">Type of claim<span class="text-danger req_text">*</span></label>
                        <select id="selClaimType" name="selClaimType" class="form-select" onchange="update()">
                            <option value="">Select</option>
                            <option value="OPD/Dispensary">OPD/Dispensary</option>
                            <option value="Cancer/Dialysis">Cancer/Dialysis</option>
                            <option value="IPD/NonCashless">IPD/NonCashless</option>
                            @*<option value="Dispensary">Dispensary</option>*@
                        </select>
                        <div class="invalid-feedback">Please enter Type of claim</div>
                    </div>

                    <!-- Hospital Visit Date -->
                    <div class="col-sm-6 col-md-4 col-lg-3 mb-3">
                        <label class="form-label">Hospital Visit Date<span class="text-danger req_text">*</span></label>
                        <input type="date" class="form-control form-control-sm" id="txtCNDDate" />
                        <div class="invalid-feedback">Please enter Date</div>
                    </div>

                    <!-- Hospital Name -->
                    <div class="col-sm-12 col-md-6 col-lg-6 mb-3">
                        <label class="form-label">Name of Hospital<span class="text-danger req_text">*</span></label>
                        <input type="text" class="form-control form-control-sm" placeholder="Enter Name of Hospital with location" id="txtCNDHospitalName" />
                        <div class="invalid-feedback">Please enter Name of Hospital</div>
                    </div>

                    <!-- Medicine -->
                    <div class="col-sm-6 col-md-4 col-lg-3 mb-3">
                        <label class="form-label">Medicine <span class="text-danger req_text">₹ *</span></label>
                        <input type="text" class="form-control form-control-sm number" placeholder="Enter Medicine (Rs.)" id="txtCNDMedicineAmount" onkeyup="calculateTotal()" />
                        <div class="invalid-feedback">Please enter Medicine</div>
                    </div>

                    <!-- Consultation -->
                    <div class="col-sm-6 col-md-4 col-lg-3 mb-3">
                        <label class="form-label">Consultation <span class="text-danger req_text">₹ *</span></label>
                        <input type="text" class="form-control form-control-sm number" placeholder="Enter Consultation (Rs.)" id="txtCNDConsultationAmount" onkeyup="calculateTotal()" />
                        <div class="invalid-feedback">Please enter Consultation</div>
                    </div>

                    <!-- Investigation -->
                    <div class="col-sm-6 col-md-4 col-lg-3 mb-3">
                        <label class="form-label">Investigation Amount <span class="text-danger req_text">₹ *</span></label>
                        <input type="text" class="form-control form-control-sm number" placeholder="Enter Investigation Amount (Rs.)" id="txtCNDInvestigationAmount" onkeyup="calculateTotal()" />
                        <div class="invalid-feedback">Please enter Investigation Amount</div>
                    </div>

                    <!-- Other Expenses -->
                    <div class="col-sm-6 col-md-4 col-lg-3 mb-3">
                        <label class="form-label">Other Expenses Amount <span class="text-danger req_text">₹ *</span></label>
                        <input type="text" class="form-control form-control-sm number" placeholder="Enter Other Amount" id="txtCNDOtherAmount" onkeyup="calculateTotal()" />
                        <div class="invalid-feedback">Please enter Other Expenses Amount</div>
                    </div>

                    <!-- Total -->
                    <div class="col-sm-6 col-md-4 col-lg-3 mb-3">
                        <label class="form-label">Total Amount <span class="text-danger req_text">₹ *</span></label>
                        <input type="text" class="form-control form-control-sm number" placeholder="Enter Total Amount" id="txtCNDTotalAmount" disabled />
                        <div class="invalid-feedback">Please enter Total Amount</div>
                    </div>

                    <!-- Add Button -->
                    <div class="col-sm-6 col-md-3 col-lg-2 align-self-end mb-3">
                        <button class="btn btn-success w-100" type="button" id="btnAddCND">
                            <i class="fa fa-plus me-1"></i>Add
                        </button>
                    </div>

                    <!-- Table -->
                    <div class="col-12 mt-3">
                        <div class="table-responsive">
                            <table id="tblCND" class="table table-bordered">
                                <thead class="table-light">
                                    <tr>
                                        <th>Hospital Visit Date</th>
                                        <th>Name of Hospital</th>
                                        <th>Medicine Amt</th>
                                        <th>Consultation Amt</th>
                                        <th>Investigation Amt</th>
                                        <th>Other Expenses Amt</th>
                                        <th>Total Amount</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td colspan="8" class="text-center">No data available</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>

                </div>


                <h4 class="form_title mt-3 shadow-2-strong">
                    Claim Document Upload
                </h4>
                <div class="row">

                    <div class="col-sm-4 col-md-5 col-lg-5 col-xl-5 mb-3">
                        <label class="form-label d-block mb-1">Document Type<span class="text-danger req_text"></span></label>
                        @if (Model.Count() > 0)
                        {
            <select id="selDocumentType" class="form-select" required>
                <option value="">Select</option>
                @foreach (var item in Model)
                {
            <option value="@item.DocumentId">@item.DocumentName</option>}
            </select> }
        else
        {
            <select id="selDocumentType" class="form-control" required>
                <option value="">Select</option>
            </select>}
                        <div class="invalid-feedback ">
                            Please Enter Document Type
                        </div>
                    </div>
                    <div class="col-sm-4 col-md-5 mb-3">
                        <label class="form-label d-block mb-1">
                            Choose file<span class="text-danger req_text"></span>
                        </label>
                        <input class="form-control" type="file" id="fileUpload" accept="application/pdf">
                    </div>
                    <div class="col-sm-3 col-md-2 col-lg-2 col-xl-1 position-relative px-0">
                        <button id="btnUpload" type="button" class=" btn btn-success position-absolute top-25 fw-bolder"><i class="fa fa-plus mr-2"></i>Add</button>
                        <button id="btnTestDocumentArray" type="button" style="display:none" class=" btn btn-success position-absolute top-25 fw-bolder"><i class="fa fa-save mr-2"></i>Add</button>
                    </div>
                    <div class="col-sm-12">
                        <table id="tblDocument" class="table table-bordered ">
                            <thead>
                                <tr>
                                    <th width="7%" class="text-center">Sr. No.</th>
                                    <th>Document Type</th>
                                    <th>Attach file </th>
                                    <th width="8%">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>

                    </div>
                </div>
                <h4 class="form_title mt-3 shadow-2-strong">
                    Bank Details
                </h4>
                <div class="row">
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">Account Holder Name<span class="text-danger req_text">*</span></label>
                        <input type="text" id="txtAccountHolderName" class="form-control form-control-sm" placeholder="Account Holder Name" required onkeypress="return  lettersOnly(event)">
                        <div class="invalid-feedback ">
                            Please Enter Account Holder Name
                        </div>
                    </div>
