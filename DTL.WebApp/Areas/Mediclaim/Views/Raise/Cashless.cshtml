                </div>

                <h4 class="form_title mt-3 shadow-2-strong">
            To be Filled By Hospital at the time of discharge
        </h4>
                <div class="row">
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3">
                        <label class="form-label d-block mb-1">
                            Date of Admission <span class="text-danger req_text">*</span>
                        </label>
                        <input type="date" id="dtDateOfAdmission" class="form-control form-control-sm" placeholder="Date of Admission" />
                        <div class="invalid-feedback">
                            Please Enter Date of Admission
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">
                            Date Of Discharge<span class="text-danger req_text">*</span>
                        </label>
                        <input type="Date" id="dtDischarge" class="form-control form-control-sm" placeholder="Date Of Birth" />
                        <div class="invalid-feedback">
                            Please Enter Date Of Discharge
                        </div>
                    </div>

                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">
                            Diagnosis <span class="text-danger req_text">*</span>
                        </label>
                        <input type="text" id="txtDiagnosis" maxlength="50" class="form-control form-control-sm" placeholder="Card Type / Illness Type" />
                        <div class="invalid-feedback">
                            Please Enter Diagnosis
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">
                            Type Of treatment <span class="text-danger req_text">*</span>
                        </label>
                        <textarea id="txtTypeOfTreatment" maxlength="250" class="form-control form-control-sm" placeholder="Treatment Type"></textarea>

                        <div class="invalid-feedback">
                            Please Enter Type Of treatment
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">
                            SignatureDischargeTime <span class="text-danger req_text">*</span>
                        </label>
                        <textarea id="txtSignatureDischargeTime" maxlength="250" class="form-control form-control-sm" placeholder="Signature Discharge"></textarea>

                        <div class="invalid-feedback">
                            Please Enter Type Of treatment
                        </div>
                    </div>

                </div>

                <h4 class="form_title mt-3 shadow-2-strong">
                    Claim Document Upload
                </h4>
                <div class="row">
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 col-md-5 col-lg-5 col-xl-5 mb-3">
                        <label class="form-label d-block mb-1">
                            Document Type<span class="text-danger req_text">*</span>
                        </label>
                        @if (Model.Count() > 0)
                        {
                            <select id="selDocumentType" class="form-select">
                                <option value="">Select</option>
                                @foreach (var item in Model)
                                {
                                    <option value="@item.DocumentId">@item.DocumentName</option>
                                }
                            </select>
                            <div class="invalid-feedback">
                                Please Enter Document Type
                            </div>
                        }
                        else
                        {
                            <select id="selDocumentType" class="form-control">
                                <option value="">Select</option>
                            </select>
                            <div class="invalid-feedback">
                                Please Enter Document Type
                            </div>
                        }
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 col-md-5 mb-3">
                        <label class="form-label d-block mb-1">
                            Choose file<span class="text-danger req_text">*</span>
                        </label>
                        <input class="form-control" type="file" id="fileUpload" accept="application/pdf">

                    </div>
                    <div class="col-sm-3 col-md-2 col-lg-2 col-xl-1 position-relative px-0">
                        @*<button type="button" class="Add_btn btn btn-brand w-100" id="btnUpload">Add</button>*@
                        <button type="button" class="btn btn-success position-absolute top-25" id="btnUpload"><i class="fa fa-plus mr-2"></i>Add</button>
                    </div>
                    <div class="col-sm-12">
                        <table id="tblDocument" class="table table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th style="width:10%">Sr. No.</th>
                                    <th style="width:40%">Document Type</th>
                                    <th style="width:30%">Attach File</th>
                                    <th style="width:20%">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>

                    </div>
                </div>
                <h4 class="form_title mt-3 shadow-2-strong">
                    Bank Details
                </h4>
                <div class="row">
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">
                            Account Holder Name<span class="text-danger req_text">*</span>
                        </label>
                        <input type="text" id="txtAccountHolderName" maxlength="50" class="form-control form-control-sm" placeholder="Account Holder Name" onkeypress="return lettersOnly(event)" />
                        <div class="invalid-feedback">
                            Please Enter Account Holder Name
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">
                            Account Number<span class="text-danger req_text">*</span>
                        </label>
                        <input type="text" id="txtAccountNumber" class="form-control form-control-sm" placeholder="Account Number" maxlength="17" onkeypress="return isNumberKey(event)" />
                        <div class="invalid-feedback">
                            Please Enter Account Number
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">
                            Bank Name<span class="text-danger req_text">*</span>
                        </label>
                        <input type="text" id="txtBankName" maxlength="150" class="form-control form-control-sm" placeholder="Bank Name" onkeypress="return lettersOnly(event)" />
                        <div class="invalid-feedback">
                            Please Enter Bank Name
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">
                            Branch Name<span class="text-danger req_text">*</span>
                        </label>
                        <input type="text" id="txtBranchName" maxlength="100" class="form-control form-control-sm" placeholder="Branch Name" onkeypress="return lettersOnly(event)" />
                        <div class="invalid-feedback">
                            Please Enter Branch Name
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">
                            BIC Code<span class="text-danger req_text">*</span>
                        </label>
                        <input type="text" id="txtBICCode" class="form-control form-control-sm" placeholder="BIC Code" maxlength="12" />
                        <div class="invalid-feedback">
                            Please Enter BIC Code
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">
                            IFSC code <span class="text-danger req_text">*</span>
                        </label>
                        <input type="text" id="txtIFSCNumber" class="form-control form-control-sm" placeholder="IFSC Code" maxlength="11" />
                        <div class="invalid-feedback">
                            Please Enter  IFSC code
                        </div>
                    </div>
                </div>
                <div class="col-md-12 mb-3 submit_area submit_area_strip" style="background: aliceblue;">
                    <button type="button" class="btn btn-brand me-2 hide_submit" id="btnSubmit" data-toggle="modal" data-target="">Submit</button>
                    <a href="/Mediclaim/Cashless" role="button" class="btn btn-cancel me-2 hide_submit">Cancel</a>
                    <button type="reset" class="btn btn-reset me-2">Reset</button>
                </div>
                <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header py-2">
                                <h5 class="modal-title">Cashless Mediclaim</h5>
                                <button type="button" class="btn-close close_btn" data-bs-dismiss="modal" aria-label="Close"> </button>
                            </div>
                            <div class="modal-body ">
                                <div class="content pt-4 pb-0">
                                    <img src="~/assets/images/success.png" class="icon_reject ">
                                    <h3 class="message-type text-blod pb-0 pt-3 ">
                                        Claim Submitted Successfully
                                    </h3>
                                    <p class="text-center">
                                        Claim Number is <b>
                                            <label id="lblClaimNumber"></label>
                                        </b>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

