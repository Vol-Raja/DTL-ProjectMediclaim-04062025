@using DTL.Model.Models
@model IEnumerable<MasterDocumentModel>
@{ ViewData["Title"] = "Cashless";
    Layout = "~/Views/Shared/_Layout.cshtml"; }

<div class="container-fluid">
    <div class="row header_section mx-0 px-0">
        <div class="col-6 position-relative ps-0">
            <span class="btn btn-light float-start mt-1 me-2 d-block d-md-none" id="menu_toggle">
                <i class="fa-solid fa-bars"></i>
            </span>
            <h3 class="mb-0">Cashless Mediclaim Form </h3>
            <span class="subTitle">(To be filled by hospital only)</span>
        </div>
        <p class="mb-0 col-6 top_link_Section">
            <a href="/">Dashboard</a><span class="px-1">/</span><a href="@Url.Action("Index","Cashless",new {Area="Mediclaim" })">Cashless Mediclaim</a><span class="px-1">/</span>  <span>Cashless Mediclaim Form</span>
        </p>
    </div>
    <div class="card border-card mt-0">
        <div class="card-body">
            <form class="g-3 mt-2 needs-validation" id="cashlessForm" action="" method="post">
                <input type="hidden" id="hdnMediClaimId" />
                <h4 class="form_title shadow-2-strong ">
                    Detail Of Hospital
                </h4>
                <div class="row">
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1" for="name">Name Of Hospital<span class="text-danger req_text">*</span></label>
                        <input type="text" id="txtNameOfHospital" maxlength="100" readonly class="form-control form-control-sm " placeholder="Name Of Hospital" />
                        <div class="invalid-feedback">
                            Please Enter Name Of Hospital
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">
                            Hospital Id<span class="text-danger req_text">*</span>
                        </label>
                        <input type="text" id="txtHospitalId" maxlength="15" readonly class="form-control form-control-sm" placeholder="Hospital Id" />
                        <div class="invalid-feedback">
                            Please Enter Hospital Id
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3">
                        <label class="form-label d-block mb-1">
                            Phone Number<span class="text-danger req_text">*</span>
                        </label>
                        <input type="text" id="txtHospitalPhoneNumber" readonly class="form-control form-control-sm" placeholder="Phone Number" />
                        <div class="invalid-feedback">
                            Please Enter Phone Number
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3">
                        <label class="form-label d-block mb-1">
                            Email Id<span class="text-danger req_text">*</span>
                        </label>
                        <input type="email" id="txtEmailId" maxlength="50" readonly class="form-control form-control-sm" placeholder="Email id" />
                        <div class="invalid-feedback">
                            Please Enter  Email Id
                        </div>
                    </div>
                    <div class="col-sm-8 mb-3">
                        <label class="form-label d-block mb-1">
                            Address<span class="text-danger req_text">*</span>
                        </label>
                        <textarea id="txtHospitalAddress" maxlength="250" readonly class="form-control form-control-sm" placeholder="Address"></textarea>
                        <div class="invalid-feedback">
                            Please Enter Address
                        </div>
                    </div>
                </div>
                <h4 class="form_title mt-3 shadow-2-strong">
                    Card Holder Details
                </h4>
                <div class="row">
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3">
                        <label class="form-label d-block mb-1">
                            Name of medical card holder<span class="text-danger req_text">*</span>
                        </label>
                        <input type="text" id="txtMedicalCardHolderName" class="form-control form-control-sm" placeholder="Name of medical card holder" onkeypress="return  lettersOnly(event)" />
                        <div class="invalid-feedback">
                            Please Enter Name of medical card holder
                        </div>

                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">
                            Medical Card number<span class="text-danger req_text">*</span>
                        </label>
                        <input type="text" id="txtMedicalCardNumber" maxlength="16" class="form-control form-control-sm" placeholder="Medical Card Number" />
                        <div class="invalid-feedback">
                            Please Enter Medical Card Number
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">
                            Card Category<span class="text-danger req_text">*</span>
                        </label>

                        <select name="" id="selCardCategory" class="form-select">
                            <option value="">Select</option>
                            <option value="General">General</option>
                            <option value="Semi-Private">Semi Private</option>
                            <option value="Private">Private</option>
                        </select>
                        <div class="invalid-feedback">
                            Please Enter Card Category
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">
                            Medical Section Page No<span class="text-danger req_text">*</span>
                        </label>
                        <input type="text" id="txtMedicalSectionPageNumber" maxlength="50" class="form-control form-control-sm" placeholder="Medical Section Page No" />
                        <div class="invalid-feedback">
                            Please Enter Medical Section Page No
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3">
                        <label class="form-label d-block mb-1">
                            Email Id<span class="text-danger req_text">*</span>
                        </label>
                        <input type="email" id="txtPatientEmailId" maxlength="50" class="form-control form-control-sm" placeholder="Email id" />
                        <div class="invalid-feedback">
                            Please Enter Email Id
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3">
                        <label class="form-label d-block mb-1" for="name">
                            PPO Number<span class="text-danger req_text">*</span>
                        </label>
                        <input type="text" id="txtPPONumber" class="form-control form-control-sm" placeholder="PPO Number" />
                        <div class="invalid-feedback">
                            Please Enter PPO Number
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">
                            Date Of Birth<span class="text-danger req_text">*</span>
                        </label>
                        <input type="Date" id="dtDateOfBirth" class="form-control form-control-sm" placeholder="Date Of Birth" />
                        <div class="invalid-feedback">
                            Please Enter Date Of Birth
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">
                            Age<span class="text-danger req_text">*</span>
                        </label>
                        <input type="number" id="txtAge" readonly class="form-control form-control-sm" placeholder="Age" />
                        <div class="invalid-feedback">
                            Please Enter Age
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3">
                        <label class="form-label d-block mb-1">
                            Mobile Number<span class="text-danger req_text">*</span>
                        </label>
                        <input type="text" id="txtPatientPhoneNumber" maxlength="10" class="form-control form-control-sm" placeholder="Mobile Number" onkeypress="return  isNumberKey(event)" />
                        <div class="invalid-feedback">
                            Please Enter Mobile Number
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3">
                        <label class="form-label d-block mb-1">Organization<span class="text-danger req_text">*</span></label>
                        @Html.DropDownList("Organization", (IEnumerable<SelectListItem>)ViewBag.DTLOfficeList, "Select", new { id = "selOrganization", @class = "form-select" })
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3">
                        <label class="form-label d-block mb-1">Department<span class="text-danger req_text">*</span></label>
                        <input type="text" id="txtDepartment" maxlength="100" class="form-control form-control-sm" placeholder="Department" />
                        <div class="invalid-feedback">
                            Please Enter Department
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3">
                        <label class="form-label d-block mb-1">
                            Designation<span class="text-danger req_text">*</span>
                        </label>
                        <input type="text" id="txtDesignation" maxlength="100" class="form-control form-control-sm" placeholder="Designation" onkeypress="return lettersOnly(event)" />
                        <div class="invalid-feedback">
                            Please Enter Designation
                        </div>
                    </div>


                    <div class="col-sm-12  mb-3">
                        <label class="form-label d-block mb-1">
                            Address<span class="text-danger req_text">*</span>
                        </label>
                        <textarea id="txtPatientAddress" maxlength="250" class="form-control form-control-sm" placeholder="Address"></textarea>
                        <div class="invalid-feedback">
                            Please Enter Address
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3">
                        <label class="form-label d-block mb-1">
                            Date of Retirement<span class="text-danger req_text">*</span>
                        </label>
                        <input type="date" id="dtDateRetirement" class="form-control form-control-sm" placeholder="Date of Retirement" />
                        <div class="invalid-feedback">
                            Please Enter  Date of Retirement
                        </div>
                    </div>
                </div>
                <h4 class="form_title mt-3 shadow-2-strong">
                    Detail Of Patients
                </h4>

                <div class="row">
                    <div class="col-sm-3  mb-3">
                        <label class="form-label d-block mb-1" for="name">
                            Name Of Patient<span class="text-danger req_text">*</span>
                        </label>
                        <input type="text" id="txtNameOfPatient" maxlength="50" class="form-control form-control-sm " placeholder="Name Of Patient" onkeypress="return lettersOnly(event)" />
                        <div class="invalid-feedback">
                            Please Enter Name Of Patient
                        </div>

                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">
                            relationship with
                            retiree<span class="text-danger req_text">*</span>
                        </label>
                        <select name="RelatioWithRetire" id="selRelatioWithRetire" class="form-select ">
                            <option value="">Select</option>
                            <option value="Self">Self</option>
                            <option value="Spouse">Spouse</option>
                            <option value="Parents">Parents</option>
                            <option value="Child">Child</option>
                        </select>
                        <div class="invalid-feedback ">
                            Please Select relationship with
                            retiree
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">
                            Date Of Birth<span class="text-danger req_text">*</span>
                        </label>
                        <input type="Date" id="dtDependantDOB" class="form-control form-control-sm" placeholder="Date Of Birth" />
                        <div class="invalid-feedback">
                            Please Enter Date Of Birth
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">
                            Age<span class="text-danger req_text">*</span>
                        </label>
                        <input type="number" id="txtDependantAge" readonly class="form-control form-control-sm" placeholder="Age" />
                        <div class="invalid-feedback">
                            Please Enter Age
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-6 col-lg-4 col-xl-3 col-md-6 col-lg-4 col-xl-3 mb-3">
                        <label class="form-label d-block mb-1">Gender</label>
                        <div class="d-flex mt-3 gender-radio">
                            <div class="form-check me-5">
                                <input class="form-check-input" type="radio" name="gender" id="rdMale" value="Male" checked="">
                                <label class="form-check-label" for="male">
                                    Male
                                </label>
                            </div>
                            <div class="form-check me-5">
                                <input class="form-check-input" type="radio" name="gender" id="rdFemale" value="Female">
                                <label class="form-check-label" for="female">
                                    Female
                                </label>
                            </div>
                            <div class="form-check">
                                <input class="form-check-input" type="radio" name="gender" id="rdOther" value="Other">
                                <label class="form-check-label" for="other">
                                    Other
                                </label>
                            </div>
                        </div>
                    </div>

                    @*<div class="col-sm-6 col-md-6 col-lg-4 col-xl-3  mb-3">
                        <label class="form-label d-block mb-1">
                            Date of Admission <span class="text-danger req_text">*</span>
                        </label>
                        <input type="date" id="dtDateOfAdmission" class="form-control form-control-sm" placeholder="Date of Admission" />
                        <div class="invalid-feedback">
                            Please Enter Date of Admission
                        </div>
                    </div>*@




