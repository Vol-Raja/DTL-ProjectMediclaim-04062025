                            <td scope="row">@Model.NonCashless.ClaimFor</td>
                        </tr>
                        @if (Model.NonCashless.Dependent != null && Model.NonCashless.ClaimFor.ToLower() == "dependent")
                        {
                            <tr>
                                <td scope="row" colspan="4" class="text-center h4">Dependent Information</td>
                            </tr>
                            <tr>

                                <th scope="row">Name</th>
                                <td scope="row">@Model.NonCashless.Dependent.Name</td>
                                <th scope="row">Date Of Birth</th>
                                <td scope="row">@Model.NonCashless.Dependent.DateOfBirth.ToString("dd-MM-yyyy")</td>
                            </tr>
                            <tr>
                              
                                <th scope="row">Age</th>
                                <td scope="row">@Model.NonCashless.Dependent.Age</td>
                                <th scope="row"> Relationship With Retiree</th>
                                <td scope="row">@Model.NonCashless.Dependent.RelationWithRetire</td>
                            </tr>
                        }
                        <tr>
                            <td scope="row" colspan="4" class="text-center h4">Bank Detail</td>
                        </tr>
                        <tr>
                            <th scope="row">Account Holder Name</th>
                            <td scope="row" colspan="3">@Model.NonCashless.AccountHolderName</td>
                        </tr>
                        <tr>
                            <th scope="row">Bank Name</th>
                            <td scope="row">@Model.NonCashless.BankName</td>
                            <th scope="row">BIC Code</th>
                            <td scope="row">@Model.NonCashless.BICCode</td>
                        </tr>
                        <tr>
                            <th scope="row">IFSC Number</th>
                            <td scope="row">@Model.NonCashless.IFSCNumber</td>
                            <th scope="row">Account Number</th>
                            <td scope="row">@Model.NonCashless.AccountNumber</td>
                        </tr>
                        <tr>
                            <td scope="row" colspan="4" class="text-center h4">Detail of Hospitalization</td>
                        </tr>
                        @foreach (var item in Model.NonCashless.OPDCNDList)
                        {
                            <tr>
                                <th scope="row">Claim Type</th>
                                <td scope="row">@Model.NonCashless.ClaimType</td>
                                <th scope="row">Date</th>
                                <td scope="row">@item.OPDCNDDate.ToString("dd/MM/yyyy")</td>
                            </tr>
                            <tr>
                                <th scope="row">Name of Hospital</th>
                                <td scope="row">@item.HospitalName</td>
                                <th scope="row">Medicine <span class="text-danger req_text">&#8377;</span></th>
                                <td scope="row">@item.MedicineAmount</td>
                            </tr>
                            <tr>
                                <th scope="row">Investigation Amount <span class="text-danger req_text">&#8377;</span></th>
                                <td scope="row">@item.InvestigationAmount</td>
                                <th scope="row">Consultation <span class="text-danger req_text">&#8377;</span></th>
                                <td scope="row">@item.ConsultationAmount</td>
                            </tr>
                            <tr>

                                <th scope="row">Other Expenses Amount <span class="text-danger req_text">&#8377;</span></th>
                                <td scope="row">@Math.Round(item.OtherAmount, 2)</td>
                                <th scope="row">Total Amount <span class="text-danger req_text">&#8377;</span></th>
                                <td scope="row">@Math.Round(item.TotalAmount, 2)</td>
                            </tr>
                        }
                        <tr>
                            <th scope="row" colspan="4" class="text-center h4">Claim Document Upload</th>
                        </tr>
                        <tr>
                            <th class="text-center">Sr. No.</th>
                            <th colspan="3">Document Type</th>
                        </tr>
                        @if (Model.NonCashless.Documents.Count() > 0)
                        {
                            var _index = 1;
                            @foreach (var item in Model.NonCashless.Documents)
                            {
                                <tr>
                                    <td class="text-center">@_index</td>
                                    <td colspan="3">

                                        <a href="#" class="pdfIcons" target='_blank'>
                                            <i class="fa fa-file-pdf" aria-hidden="true"></i>
                                            @item.FileName
                                        </a>
                                    </td>
                                </tr>
                                _index = _index + 1;
                            }
                        }
                        else
                        {
                            <tr>
                                <td class="text-center" colspan="4">NO Document Found</td>
                            </tr>
                        }

                        <tr>
                            <td scope="row" colspan="4" class="text-center h4">Claim Approval status</td>
                        </tr>
                        <tr>
                            <th scope="row">
                                Approve Amount<span class="text-danger req_text">&#8377;</span>
                            </th>
                            <td scope="row">@Model.NonCashless.ApprovedAmount</td>
                            <th scope="row">
                                In Words
                                <span class="text-danger req_text">&#8377;</span>
                            </th>
                            <td scope="row"><label id="lblAmountInWords"></label></td>
                        </tr>
                        <tr>
                            @if (ViewBag.Designation.ToLower() == "da1" || ViewBag.Designation.ToLower() == "da2")
                            {
                                <th scope="row">Dealing Asistant Remark</th>
                                <td scope="row">@Model.NonCashless.DealingAssistanceRemark</td>
                            }
                            @if (ViewBag.Designation.ToLower() == "aso")
                            {
                                <th scope="row">Aso Remark</th>
                                <td scope="row">@Model.NonCashless.ASORemark</td>
                            }
                        </tr>
                        <tr>
                            @if (ViewBag.Designation.ToLower() == "so")
                            {
                                <th scope="row">So Remark</th>
                                <td scope="row">@Model.NonCashless.SORemark</td>
                            }
                            <th scope="row">Remark</th>
                            <td scope="row">@Model.NonCashless.Remark</td>
                        </tr>
                        <tr>
                            <th scope="row">Claim Status</th>
                            @if (Model.NonCashless.ClaimStatusId == 1)
                            {
                                <td scope="row">Pending</td>
                            }
                            else if (Model.NonCashless.ClaimStatusId == 2)
                            {
                                <td scope="row">Approved</td>
                            }
                            else
                            {
                                <td scope="row">Rejected</td>
                            }
                        </tr>
                    </tbody>
                </table>
            </div>
        }
        else { }
    </div>
</div>
<script type="text/javascript">
    $('#print_preview_nonashless_raise').hide();

    $('#printMe').click(function () {
        $('#print_preview_nonashless_raise').removeClass('show');
        $('#print_preview').addClass('show');
        window.print();
    });

    $('#printMeNonCashlessRaise').click(function () {
        $('#print_preview').removeClass('show');
        $('#print_preview_nonashless_raise').addClass('show');
        window.print();
    });

</script>

<script src="~/assets/js/tab.js"></script>
<script src="~/js/numbertoword.js"></script>
<script>
    $(document).ready(function () {
        var numberToWord = new NumberToWord();
        $('#lblAmountInWords').text(numberToWord.ToWord($('#hdnNonCashlessAproveAmount').val()))
    });
</script>
