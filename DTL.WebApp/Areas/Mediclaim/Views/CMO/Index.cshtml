
@{ ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml"; }
<div class="container-fluid">
    <div class="row header_section mx-0 px-0 border_bottom">
        <div class="col-5 position-relative ps-0">
            <span class="btn btn-light float-start mt-1 me-2 d-block d-md-none" id="menu_toggle">
                <i class="fa-solid fa-bars"></i>
            </span>
            <h3 class="mb-0">  Cheif Medical Officer Dashboard </h3>
        </div>
        <p class="mb-0 col-7 top_link_Section"><span class="">Dashboard </span></p>
    </div>
    <div class="container-fluid pd-x-0 pd-lg-x-10 pd-xl-x-0">
        <div class="row row-xs pt-3">
            <div class="col-sm-6 col-lg-3 mt-2">
                <div class="card dashboard background-orange">
                    <a href="@Url.Action("CashlessClaims","Processing",new { Area="Mediclaim"})">
                        <div class="card-body">
                            <div class="d-flex d-lg-block d-xl-flex align-items-end">
                                <h5 class="tx-normal tx-rubik mg-b-0 mg-r-5 lh-1 box_title">Mediclaim Processing</h5>
                                <i class="fa-solid fa-tachometer"></i>
                            </div>
                        </div>
                    </a>
                </div>
            </div><!-- col -->
            <div class="col-sm-6 col-lg-3 mt-2">
                <div class="card dashboard background-yellow">
                    <a href="@Url.Action("Vouchers","cashless",new { Area="Mediclaim"})">
                        <div class="card-body">
                            <div class="d-flex d-lg-block d-xl-flex align-items-end">
                                <h5 class="tx-normal tx-rubik mg-b-0 mg-r-5 lh-1 box_title">Voucher Generation</h5>
                                <i class="fa-solid fa-ticket"></i>
                            </div>
                        </div>
                    </a>
                </div>
            </div><!-- col -->
            <div class="col-sm-6 col-lg-3 mt-2">
                <div class="card dashboard background-red">
                    <a href="@Url.Action("EmpanelledHospitalList","EmpanelledHospitals",new { Area="Mediclaim"})">
                        <div class="card-body">
                            <div class="d-flex d-lg-block d-xl-flex align-items-end">
                                <h5 class="tx-normal tx-rubik mg-b-0 mg-r-5 lh-1 box_title">Empanelled Hospital List</h5>
                                <i class="fa-solid fa-tachometer"></i>
                            </div>
                        </div>
                    </a>
                </div>
            </div><!-- col -->
            <div class="col-sm-6 col-lg-3 mt-2">
                <div class="card dashboard background-purple">
                    <a href="@Url.Action("CGHSRateCard","CghsRateCard", new { Area = "Mediclaim"})">
                        <div class="card-body">
                            <div class="d-flex d-lg-block d-xl-flex align-items-end">
                                <h5 class="tx-normal tx-rubik mg-b-0 mg-r-5 lh-1 box_title">CGHS Rate card List</h5>
                                <i class="fa-solid fa-tachometer"></i>
                            </div>
                        </div>
                    </a>
                </div>
            </div><!-- col -->
            <div class="col-sm-6 col-lg-3 mt-2">
                <div class="card dashboard background-green">
                    <a href="@Url.Action("OPDMedicalPageDetails","MedicalPageDetail", new { Area = "Mediclaim"})">
                        <div class="card-body">
                            <div class="d-flex d-lg-block d-xl-flex align-items-end">
                                <h5 class="tx-normal tx-rubik mg-b-0 mg-r-5 lh-1 box_title">Medical Page Details</h5>
                                <i class="fa-solid fa-file "></i>
                            </div>
                        </div>
                    </a>
                </div>
            </div><!-- col -->
            <div class="col-sm-6 col-lg-3 mt-2">
            <div class="card dashboard background-orange">
                <a href="@Url.Action("CashlessCreditletter","Processing",new { Area="Mediclaim"})">
                    <div class="card-body">
                        <div class="d-flex d-lg-block d-xl-flex align-items-end">
                            <h5 class="tx-normal tx-rubik mg-b-0 mg-r-5 lh-1 box_title">Credit Letter</h5>
                            <i class="fa-solid fa-tachometer"></i>
                        </div>
                    </div>
                </a>
            </div>
        </div> <!-- col -->
        </div>
    </div>
</div>