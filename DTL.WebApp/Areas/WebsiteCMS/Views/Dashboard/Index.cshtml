@using DTL.WebApp.Areas.WebsiteCMS
@{
    ViewData["Title"] = "CMS Dashboard Management";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="container-fluid">
 
    <div class="row header_section mx-0 px-0 border_bottom">
        <div class="col-6 position-relative ps-0">
            <span class="btn btn-light float-start mt-1 me-2 d-block d-md-none" id="menu_toggle">
                <i class="fa-solid fa-bars"></i>
            </span>
            <h3 class="mb-0">CMS Dashboard</h3>
        </div>
        <p class="mb-0 col-6 top_link_Section">
            <a href="@Url.Action("Index","Home",new { Area="" })"> Dashboard</a><span class="px-1">/</span><span>CMS Dashboard</span>
        </p>
    </div>
    <div class="container-fluid pd-x-0 pd-lg-x-10 pd-xl-x-0">
        <div class="row row-xs">
            <div class="col-sm-6 col-lg-3 mt-2">
                <div class="card dashboard bg-warning">
                    <a href="@Url.Action(StringConstants.DASHBOARD,StringConstants.CMS_About)">
                        <div class="card-body">
                            <div class="d-flex d-lg-block d-xl-flex align-items-end">
                                <h5 class="tx-normal tx-rubik mg-b-0 mg-r-5 lh-1 box_title"> About us</h5>
